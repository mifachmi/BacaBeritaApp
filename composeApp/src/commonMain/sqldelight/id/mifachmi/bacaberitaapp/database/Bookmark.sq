CREATE TABLE bookmark (
  id TEXT NOT NULL PRIMARY KEY,
  title TEXT NOT NULL,
  publishedTime TEXT NOT NULL,
  imageUrl TEXT,
  description TEXT
);

selectAll:
SELECT * FROM bookmark;

insertBookmark:
INSERT OR REPLACE INTO bookmark(id, title, publishedTime, imageUrl, description)
VALUES (?, ?, ?, ?, ?);

deleteBookmark:
DELETE FROM bookmark WHERE title = ?;